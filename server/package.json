{
	"name": "server",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "nodemon --ignore src/public/static --exec 'ts-node -r ./node_modules/dotenv/config ./src/bin/www.ts'",
		"format": "prettier \"./**/*.js\" --write",
		"build": "tsc",
		"start": "node -r ./node_modules/dotenv/config dist/bin/www.js"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"csv-parser": "^2.3.3",
		"debug": "~2.6.9",
		"dotenv": "^8.2.0",
		"express": "~4.16.1",
		"http-errors": "~1.6.3",
		"mongoose": "^6.0.10",
		"mongoose-fuzzy-searching": "^1.3.1",
		"morgan": "~1.9.1",
		"node-fetch": "^2.6.0",
		"youtube-dl": "^3.0.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.10.5",
		"@babel/core": "^7.11.4",
		"@babel/node": "^7.10.5",
		"@babel/plugin-transform-runtime": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.10",
		"@types/debug": "^4.1.5",
		"@types/express": "^4.17.11",
		"@types/http-errors": "^1.8.0",
		"@types/morgan": "^1.9.2",
		"@types/node": "^14.14.31",
		"@types/node-fetch": "^2.5.8",
		"@types/youtube-dl": "^3.0.0",
		"nodemon": "^2.0.4",
		"prettier": "^1.19.1",
		"ts-node": "^10.3.0",
		"tslint": "^6.1.3",
		"typescript": "^4.2.2"
	},
	"babel": {
		"presets": [
			"@babel/preset-env"
		],
		"plugins": [
			"@babel/plugin-transform-runtime"
		],
		"ignore": [
			"../public/static"
		]
	}
}
