{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node -r ./node_modules/dotenv/config ./dist",
    "format": "prettier -w .",
    "lint": "eslint ."
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "./node_modules/.bin/ts-node -r ./node_modules/dotenv/config ./src"
  },
  "dependencies": {
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.3",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongoose": "^6.0.12",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.0",
    "youtube-dl": "^3.4.1"
  },
  "devDependencies": {
    "@types/config": "^0.0.39",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
    "@types/youtube-dl": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.3.2",
    "ts-node": "^10.3.0",
    "typescript": "^4.2.2"
  }
}
